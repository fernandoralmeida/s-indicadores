<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Indicadores.net - @ViewData["Title"]</title>
    <link rel="shortcut icon" href="~/favicon.ico" />
    <link rel="stylesheet" href="~/lib/google/material-symbols-sharp.css" />
    <link rel="stylesheet" href="~/lib/materialize/css/materialize.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/apexcharts/apexcharts.css" />
    <script src="https://cdn.jsdelivr.net/npm/ol@v8.2.0/dist/ol.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/ol@v8.2.0/ol.css">
</head>

<body id="_smooth" class="bg-color-light">
    <div class="blackboxload">
        <div id="loader" class="row div-center glass-80 z-depth-2">
            <div class="preloader-wrapper big active">
                <div class="spinner-layer spinner-blue-only">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--<div class="bg-image-glass"></div>-->
    <main style="margin-bottom:20px;">
        @RenderBody()
    </main>
    <partial name="_StripColors" />
    <footer class="page-footer">
        <div class="container">
            <div class="row">
                <div class="col l6 s12">
                    <h5 class="white-text">indicadores.net</h5>
                    <p class="grey-text text-lighten-4">O jeito fácil e simplificado para consultar indicadores
                        empresariais dos municípios!</p>
                </div>
                <div class="col l4 offset-l2 s12">
                    <h5 class="white-text"><a class="grey-text text-lighten-4 right" href="#_smooth"
                            title="Ir para o Topo da pagina"><i class="material-icons">stat_3</i></a></h5>
                    <ul>
                        <li><a class="grey-text text-lighten-3"
                                href="https://www.linkedin.com/in/fernando-roberto-almeida/" target="_blank"><img
                                    src="~/svg/linkedin.svg" alt="linkedin" style="width: 28px;"></a></li>
                        <li><a class="grey-text text-lighten-3" href="https://github.com/fernandoralmeida/s-indicadores"
                                target="_blank"><img src="~/svg/github.svg" style="width: 28px;" alt="github"></a></li>
                        <li><a class="grey-text text-lighten-3" href="#!" target="_blank"><img src="~/svg/instagram.svg"
                                    style="width: 28px;" alt="instagram"></a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            <div class="container">
                &copy; 2023 - FRA
                <a class="grey-text text-lighten-4 right" asp-page="/Privacy">Politica de Privacidade</a>
            </div>
        </div>
    </footer>

    <script type="text/javascript" src="~/lib/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/materialize/js/materialize.min.js"></script>
    <script type="text/javascript" src="~/lib/apexcharts/apexcharts.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/js/bgimages.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)

</body>

</html>